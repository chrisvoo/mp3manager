{ "presets": [
    ["env", {
      "targets": { "node": "current" },
      "modules": false,
      "useBuiltIns": true,
      "debug": true }]
  ],
  "env": {
    "test": {
      "presets": [["env"]]
    },
    "development": {
      "presets": [["env"]]
    }
  },
  "plugins": [
    ["transform-runtime", {
      "polyfill": false,
      "regenerator": true
    }],
    "transform-object-rest-spread"
  ]
}
